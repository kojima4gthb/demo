<!--h1>ログイン画面</h1-->

<%= form_tag(login_path, id: 'form_login', onload: 'resetPass()') do %>

    <br />
    <table>
      <tr>
        <td class="main_dummy">
        </td>
        <td>
        </td>
      </tr>
      <tr>
        <td></td>
        <td>
          <table id="login_form">
            <tr>
              <td>
                <table>
                  <tr>
                    <td><%= label_tag :name, 'ユーザー名： ' %></td>
                    <td><%= label_tag :name, current_user_name %></td>
                  </tr>
                </table>
              </td>
            </tr>

            <tr>
              <td>
                <table>
                  <tr>
                    <td><%= label_tag :pass, 'パスワード： ' %></td>
                    <td><%= text_field_tag :disp, '', readonly: true %></td>
                    <td><%= reset_tag 'リセット', onclick: 'resetPass()' %></td>
                    <td><%= flash[:alert] %></td>
                    <td><%= hidden_field_tag :pass, params[:pass] %></td>
                  </tr>
                </table>
              </td>
            </tr>

            <tr>
              <td>
                <ul class="input_list">
                  <li>
                    <a class="i0" onclick="inputString(0)">
                      <img alt="0" src="data:image/jpeg;base64," />
                    </a>
                  </li>
                  <li>
                    <a class="i8" onclick="inputString(8)">
                      <img alt="8" src="data:image/jpeg;base64," />
                    </a>
                  </li>
                  <li>
                    <a class="i9" onclick="inputString(9)" >
                      <img alt="9" src="data:image/jpeg;base64," />
                    </a>
                  </li>
                  <li>
                    <a class="i10" onclick="inputString(10)" >
                      <img alt="10" src="data:image/jpeg;base64," />
                    </a>
                  </li>
                  <li>
                    <a class="i12" onclick="inputString(12)" >
                      <img alt="12" src="data:image/jpeg;base64," />
                    </a>
                  </li>
                  <li>
                    <a class="i14" onclick="inputString(14)" >
                      <img alt="14" src="data:image/jpeg;base64," />
                    </a>
                  </li>
                </ul>
              </td>
            </tr>
            <tr>
              <td><%= submit_tag 'ログイン' %></td>
            </tr>
          </table>

        </td>
      </tr>
    </table>

<% end %>


<br>
<a href="#"><i class="fa fa-key fa-fw fa-3x"></i></a>
<br>
<a href="#"><i class="fa fa-jump fa-fw fa-3x"></i></a>
<br>
<a href="#"><i class="fa fa-moon-o fa-fw fa-3x"></i></a>


