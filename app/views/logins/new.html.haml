= form_tag login_path, id: 'form_login', onload: 'resetPass()' do
  %div 文字パネルでパスワードを入力してください
  = label_tag :name, 'ユーザー名： '
  = label_tag :name, current_user_name
  %br
  = label_tag :pass, 'パスワード： '
  = text_field_tag :disp, '', readonly: true
  = reset_tag 'リセット', onclick: 'resetPass()'
  = flash[:alert]
  = hidden_field_tag :pass, params[:pass]
  %br
  %br
  %div.input-panel
    %ul.input-list
      %li
        %a.i0{onclick: 'inputString(0)'}
          %img{alt: '0', src: 'data:image/png;base64,'}/
      %li
        %a.i8{onclick: 'inputString(8)'}
          %img{alt: '8', src: 'data:image/png;base64,'}/
      %li
        %a.i9{onclick: 'inputString(9)'}
          %img{alt: '9', src: 'data:image/png;base64,'}/
      %li
        %a.i10{onclick: 'inputString(10)'}
          %img{alt: '10', src: 'data:image/png;base64,'}/
      %li
        %a.i12{onclick: 'inputString(12)'}
          %img{alt: '12', src: 'data:image/png;base64,'}/
      %li
        %a.i14{onclick: 'inputString(14)'}
          %img{alt: '14', src: 'data:image/png;base64,'}/
  %br
  %div= submit_tag 'ログイン'
